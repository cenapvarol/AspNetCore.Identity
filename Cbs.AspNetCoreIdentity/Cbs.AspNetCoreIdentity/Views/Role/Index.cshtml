@model List<AppRole>
@{
    Layout = "_Layout";
}

<a asp-action="Create" asp-controller="Role" class="d-inline-block my-2">Yeni rol ekle </a>
<table class="table table-hovered table-bordered table-sm">
    <thead>
        <tr>
            <th>Name</th>
            <th>Create Date</th>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.CreatedTime</td>
            </tr>
        }
    </tbody>
</table>